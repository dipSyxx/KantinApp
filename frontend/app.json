{
  "expo": {
    "name": "KantinApp",
    "slug": "kantinapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash-screen-icons/splash-icon-light.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "kantinapp",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "no.hkskole.kantinapp",
      "icon": {
        "light": "./assets/iconsIos/light-ios.png",
        "dark": "./assets/iconsIos/dark-ios.png",
        "tinted": "./assets/iconsIos/tinted-ios.png"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon/android-adaptive-icon.png",
        "backgroundColor": "#1B7A3D"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "softwareKeyboardLayoutMode": "resize",
      "package": "no.hkskole.kantinapp"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#1B7A3D",
          "image": "./assets/splash-screen-icons/splash-icon-light.png",
          "dark": {
            "image": "./assets/splash-screen-icons/splash-icon-dark.png",
            "backgroundColor": "#1B7A3D"
          },
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5b3c130d-a0ee-48c5-aaa8-e50f219cb0a0"
      }
    },
    "owner": "yuriix"
  }
}
